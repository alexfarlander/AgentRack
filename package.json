{
    "name": "agentrack",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "npm run build -w packages/shared && npm run build -w apps/web",
        "start": "PORT=8080 node .next/standalone/apps/web/server.js",
        "web:dev": "npm run dev -w apps/web",
        "web:build": "npm run build -w apps/web",
        "functions:build": "npm run build -w apps/functions",
        "functions:dev": "npm run serve -w apps/functions"
    },
    "devDependencies": {
        "typescript": "^5.9.3"
    }
}